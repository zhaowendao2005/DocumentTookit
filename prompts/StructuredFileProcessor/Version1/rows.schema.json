{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["rows"],
  "properties": {
    "rows": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["编号", "问题", "答案", "答题人", "专业"],
        "properties": {
          "编号": { "type": "integer", "minimum": 1 },
          "问题": { "type": "string", "minLength": 0, "maxLength": 500 },
          "答案": { "type": "string", "minLength": 0 },
          "答题人": { "type": "string", "minLength": 0, "maxLength": 100 },
          "专业": { "type": "string", "minLength": 0, "maxLength": 100 }
        }
      }
    }
  }
}


